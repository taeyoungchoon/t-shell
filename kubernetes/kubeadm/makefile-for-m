help:
	@cat makefile-for-m | grep ^[a-z]
start:
	vagrant up
check:
	@echo - box list
	@vagrant box list --machine-readable | grep box-name | awk -F, '{ print $$4 }'

	@echo - status
	@vagrant status --machine-readable | grep state-human-short | awk -F, '{ print $$2, "is", $$4}'
ssh:
	vagrant ssh
stop:
	vagrant halt 
clean:
	vagrant destroy -f 
